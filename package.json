{
  "name": "the-institute",
  "version": "1.0.0",
  "description": "MVP for a new and improved Chet Holmes Institute UX",
  "private": true,
  "scripts": {
    "dev": "webpack -d & nodemon server.js",
    "test-client": "jest --config=./test-config/jest.config.js ./src/client",
    "test-server": "jest --config=./test-config/jest.config.js --testEnvironment=node ./src/server ",
    "generate-seed": "node ./database/seed/index.js --amount=10000000 | lzop -c > ./database/seed/seed.lzo && lzop -dc ./database/seed/seed.lzo > ./database/seed/seed.json",
    "insert-seed": "lzop -dc ./database/seed/seed.lzo | node ./database/seed/insertSeed.js && echo 'insertion successful!'",
    "unzip": "lzop -dc ./database/seedData/seed.lzo > ./database/seedData/seed.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jordanstholmes/the-institute.git"
  },
  "author": "Jordan Holmes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jordanstholmes/the-institute/issues"
  },
  "homepage": "https://github.com/jordanstholmes/the-institute#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.9.2",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^24.6.0",
    "jest-babel": "^1.0.1",
    "redux-devtools": "^3.4.2",
    "supertest": "^4.0.2",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-redux": "^0.2.1",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "fetch-mock": "^7.3.1",
    "isomorphic-fetch": "^2.2.1",
    "mongoose": "^5.3.14",
